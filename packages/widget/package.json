{
  "name": "screenshotter",
  "version": "0.1.6",
  "description": "React widget for in-app screenshot capture with direct browser downloads.",
  "keywords": [
    "screenshot",
    "capture",
    "widget",
    "react",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zohaibarsalan/screenshotter.git",
    "directory": "packages/widget"
  },
  "bugs": {
    "url": "https://github.com/zohaibarsalan/screenshotter/issues"
  },
  "homepage": "https://github.com/zohaibarsalan/screenshotter#readme",
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "pnpm run build",
    "prebuild": "pnpm --filter @screenshotter/protocol run build",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "pretest": "pnpm --filter @screenshotter/protocol run build",
    "test": "vitest run --config vitest.config.ts"
  },
  "dependencies": {
    "@screenshotter/protocol": "workspace:*",
    "html-to-image": "^1.11.13",
    "html2canvas-pro": "^1.5.13"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
